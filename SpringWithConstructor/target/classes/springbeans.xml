<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">


<beans>          <!--beans means multiple beans -->

	<bean id="reg" class="com.entity.Register">  <!-- bean means single bean -->

		<constructor-arg index="0" value="9037634520"></constructor-arg>  <!-- in constructor-arg give same index as class Register set variables -->
		<constructor-arg index="1" value="parth shinde"></constructor-arg> <!-- in constructor-arg index always start from 0 -->
		<constructor-arg index="2" value="parthsh@gmail.com"></constructor-arg>
		<constructor-arg index="3" value="M"></constructor-arg>
		<constructor-arg index="4" value="33"></constructor-arg>
		<constructor-arg index="5" value="parth123"></constructor-arg>
		<constructor-arg index="6" ref="lg"></constructor-arg>

	</bean>
	<bean id="lg" class="com.entity.Login">
		<constructor-arg index="0" value="parthsh@gmail.com"></constructor-arg>
		<constructor-arg index="1" value="parth123"></constructor-arg>
	</bean>



</beans>   